{
  "name": "integration-tests",
  "version": "1.0.0",
  "description": "Integration tests for the application using Playwright",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "test:ui-chromium": "playwright test --project=ui-chromium",
    "test:api": "playwright test --project=api",
    "test:combined": "playwright test --project=combined-chromium",
    "report": "playwright show-report"
  },
  "keywords": [
    "test",
    "e2e",
    "playwright"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
